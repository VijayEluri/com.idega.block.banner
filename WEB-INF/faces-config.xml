<?xml version="1.0" encoding="UTF-8"?>

<faces-config 
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<component>
		<component-class>com.idega.block.banner.presentation.ad.BannerAdEditor</component-class>
		<component-type>bannerAdEditor</component-type>
		<component-extension>
			<idegaweb-info>
				<builder-visible>true</builder-visible>
				<object-type>jsf.uicomponent</object-type>
      		</idegaweb-info>
		</component-extension>
	</component>
	<component>
		<component-class>com.idega.block.banner.presentation.ad.BannerAd</component-class>
		<component-type>bannerAd</component-type>
		<property>
			<display-name>region</display-name>
			<property-name>regionName</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>category</display-name>
			<property-name>category</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<component-extension>
			<idegaweb-info>
				<builder-visible>true</builder-visible>
				<object-type>jsf.uicomponent</object-type>
      		</idegaweb-info>
		</component-extension>
	</component>

</faces-config>